{
  "schema_version": 1,
  "mixin": [
    "stb.mixins.json",
    "stb-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "stb",
    "version": "${version}",
    "metadata": {
      "name": "Sneak Through Berries",
      "description": "Lets you sneak through Sweet Berry Bushes without taking damage",
      "contributors": {
        "Trikzon": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/sneak-through-berries",
        "issues": "https://github.com/trikzon/sneak-through-berries/issues",
        "sources": "https://github.com/trikzon/sneak-through-berries"
      },
      "license": "MIT",
      "icon": "assets/stb/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "client_init": [
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18"
      }
    ]
  }
}
